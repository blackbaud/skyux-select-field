{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-select-field",
  "mode": "easy",
  "compileMode": "aot",
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "pipelineSettings": {
    "vsts": {
      "testSettings": {
        "e2e": {
          "browserSet": false
        },
        "unit": {
          "browserSet": false
        }
      }
    }
  },
  "app": {
    "styles": [
      "src/app/visual-styles.scss",
      "@skyux/docs-tools/css/docs-tools.css"
    ]
  },
  "host": {
    "frameOptions": {
      "none": true
    },
    "url": "https://developer.blackbaud.com"
  },
  "omnibar": {
    "experimental": true
  },
  "params":{
    "svcid": {
      "value": "skyux"
    }
  }
}
